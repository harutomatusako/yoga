イベント登録

<%= form_with model: @event, url: events_path, local:true do |f| %>
イベント名
<%= f.text_field :title %>
開始日時
<%= f.datetime_select :date_from, {use_month_numbers:true,minute_step:5}, class: 'form-control' %>
終了日時
<%= f.datetime_select :date_to , {use_month_numbers:true}, class: 'form-control' %>
場所
県
<%= f.select :location, Event.locations.keys, {} %>
市町村
<%= f.text_field :city %>
公園名
<%= f.text_field :park %>
内容
<%= f.text_area :detail %>
イメージ写真
<%= f.attachment_field :image %>

<%= f.submit '新規登録' %>

<% end %>
